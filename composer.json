{
    "name": "misterion/ko-worker",
    "type": "library",
    "description": "Ko-worker project is a base to develop amqp based message dispatchers",
    "keywords": ["pcntl", "fork", "background", "posix", "cli", "utility", "rpc", "amqp", "queue", "parallel"],
    "homepage": "https://github.com/misterion/ko-worker",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Bondarenko",
            "email": "misterionkell@gmail.com"
        },
        {
            "name": "Vadim Sabirov",
            "email": "pr0head@gmail.com"
        }
    ],
    "suggest": {
        "ext-proctitle": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-amqp": ">=1.4.0"
    },
    "require": {
        "php": ">=5.4.0",
        "misterion/ko-process": "~0.4",
        "symfony/yaml": "^3.0",
        "ulrichsg/getopt-php": "~2.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "empi89/php-amqp-stubs": "dev-master"
    },
    "bin": ["ko", "ko-package"],
    "autoload": {
        "psr-0": {
            "Ko": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ko\\Examples\\": "examples/"
        }
    }
}
